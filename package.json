{
  "name": "gen-query",
  "version": "1.2.12",
  "description": "Query with tanstack using generics",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rlopezb/gen-query.git",
    "homepage": "https://github.com/rlopezb/gen-query"
  },
  "author": "Rafael LÃ³pez Benavente",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@hebilicious/vue-query-nuxt": "^0.3.0",
    "@nuxt/kit": "^3.17.3",
    "@tanstack/vue-query": "^5.76.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.1",
    "@nuxt/eslint-config": "^1.3.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.3",
    "@nuxt/test-utils": "^3.18.0",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.3",
    "typescript": "~5.8.3",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10"
  }
}
